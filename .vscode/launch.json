{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "aws-sls",
      "request": "launch",
      "runtimeArgs": ["run", "dev"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "name": "aws-sls-no-auth",
      "request": "launch",
      "runtimeArgs": ["run", "dev:no-auth"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    }
  ]
}
